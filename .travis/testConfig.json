{
  "subatomic": {
    "commandPrefix" : "sub",
    "openshiftHost" : "",
    "gluon": {
      "baseUrl": "http://localhost:8080"
    },

  // lifecycle configuration
  "lifecycles": {
    "push": {
      "configuration": {
        "emoji-style": "default",
        "show-statuses-on-push": true,
        "build": {
          "style": "decorator"
        },
        "fingerprints": {
          "about-hint": false,
          "render-unchanged": true,
          "style": "fingerprint-inline"
        }
      }
    },
    "pull_request": {
      "configuration": {
        "emoji-style": "default"
      }
    }
  },
  "fingerprints": {
    "data": {
    }
  }
}