{
  "openshiftTemplate": "subatomic-spring-boot-1x-template",
  "requiredEnvironmentVariables": [
    {
      "name": "DOTNET_STARTUP_PROJECT",
      "description": "starter project location",
      "setter": "DotNet/setStartupProject"
    },
    {
      "name": "DOTNET_RESTORE_SOURCES",
      "description": "restore sources",
      "setter": "DotNet/setRestoreSources"
    }
  ],
  "buildConfig": {
    "imageStream": "jdk8-maven3-newrelic-subatomic:2.0"
  },
  "jenkinsfile": "maven-spring-boot"
}
